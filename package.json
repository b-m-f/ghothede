{
  "name": "gotede",
  "version": "1.5.8",
  "description": "Quick setup script for Ghost theme development environment with docker ",
  "main": "src/main.js",
  "bin": {
    "gotede": "./bin/gotede"
  },
  "scripts": {
    "watch": "rollup -c -w",
    "build": "cp -r src/input bin/input && cp -r src/docker-mount bin/docker-mount &&  rollup -c",
    "start": "npm run watch",
    "prepare": "npm run build"
  },
  "author": "b-m-f <max@ehlers.berlin>",
  "license": "MIT",
  "dependencies": {
    "handlebars": "^4.0.12",
    "prompt": "^1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/b-m-f/gotede.git"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "devDependencies": {
    "rollup": "^0.65.2",
    "rollup-plugin-commonjs": "^9.1.6",
    "rollup-plugin-node-resolve": "^3.3.0"
  }
}
